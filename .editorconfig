# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf

[**.js]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true

[*.{css,scss}]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true

[**.json]
indent_style = space
indent_size = 2
